{
  "name": "vine-api",
  "version": "1.0.0",
  "description": "Unofficial Vine API wrapper for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "commonjs",
  "scripts": {
    "test": "jest --coverage",
    "coverage": "codecov",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "vine",
    "api",
    "wrapper",
    "video",
    "social"
  ],
  "author": "Brad",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "jsdocs": "^1.0.0"
  },
  "devDependencies": {
    "codecov": "^3.8.3",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
